
<%= simple_form_for(@trade) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :currency %>
    <%= f.input :deposit %>
    <%= f.input :expiration %>
    <%= f.input :status %>
    <%= f.input :tradetype %>
    <%= f.input :entry %>
    <%= f.input :rate %>
    <%= f.input :return %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
